@inject BaseAppState AppState
@inject NavigationManager NavMan

<MudCard>
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h3" Align="Align.Center" >@Title</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardMedia Image="https://pdstorageaccountname.blob.core.windows.net/pdblobcontainer/PluginDemocracyImage.png"/>
    <MudCardContent>
        <MudText Typo="Typo.body2">@Body</MudText>
    </MudCardContent>
    <MudCardActions>
        <MudIconButton Icon="@Icons.Material.Filled.Home" OnClick="@(() => NavMan.NavigateTo("/"))"/>
    </MudCardActions>
</MudCard>
